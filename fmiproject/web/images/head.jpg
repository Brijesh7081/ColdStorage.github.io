}

.coi-consent-banner__category-expander:focus + .coi-consent-banner__name-container {
  outline: solid 1px #42b0d5;
}

.coi-consent-banner__switch-container {
  margin-left: 10px;
  position: absolute;
  top: 0;
  right: 0;
}

.coi-checkboxes {
  position: relative;
  display: flex;
}

