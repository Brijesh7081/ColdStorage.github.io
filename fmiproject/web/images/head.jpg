  max-width: 100%;
  max-height: 100%;
  background: #fff;
  overflow-x: hidden;
  border-radius: 5px;
}

.coi-banner__wrapper::-webkit-scrollbar-thumb {
  background-color: #ccc !important;
  border-radius: 5px;
}

.coi-banner__wrapper::-webkit-scrollbar-track {
  background-color: none;
}

.coi-banner__wrapper::-webkit-scrollbar {
  width: 6px;
  background-color: #f5f5f5;
  border-radius: 2px;
}

.coi-banner__page {
  flex: 1;
